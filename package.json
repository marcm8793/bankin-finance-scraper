{
  "name": "bankin-finance-scraper",
  "version": "1.0.0",
  "description": "Script d'automatisation pour Bankin avec Puppeteer en TypeScript",
  "main": "dist/index.js",
  "scripts": {
    "build": "rm -rf dist && tsc",
    "start": "npm run build && node dist/index.js",
    "start:prod": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "dev:watch": "ts-node-dev --respawn --transpile-only src/index.ts",
    "dev:debug": "ts-node --inspect src/index.ts",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "puppeteer",
    "bankin",
    "automation",
    "scraping",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "discord.js": "^14.19.3",
    "dotenv": "^16.3.1",
    "puppeteer": "^24.9.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "nodemon": "^3.0.2",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
